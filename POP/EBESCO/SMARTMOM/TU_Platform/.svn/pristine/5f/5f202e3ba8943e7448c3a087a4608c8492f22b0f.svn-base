			, SHIFT_CD
			, RESOURCE_CD
			, START_TIME
			, END_TIME
			, APPLY_DATE
			, WORK_PERSON_CNT
			, DESCRIPTION
			, CREATE_DATE
			, CREATE_BY
			, UPDATE_DATE
			, UPDATE_BY
			, TID
			, WORK_INDR_PERSON_CNT
        	)
		VALUES
            ( #{divisionCd, jdbcType=VARCHAR}
