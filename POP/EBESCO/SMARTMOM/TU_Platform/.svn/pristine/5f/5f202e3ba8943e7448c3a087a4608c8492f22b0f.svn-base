           AND S.SHIFT_CD IN (SELECT SHIFT_CD
					          FROM   MOM_SHIFT_CODE C
					          WHERE  C.DIVISION_CD = S.DIVISION_CD
					          AND    C.COMPANY_CD = S.COMPANY_CD
					          AND    C.SHIFT_TYPE = #{shiftType, jdbcType=VARCHAR}
					          AND	 C.USE_YN = 'Y')
           AND S.SHIFT_CD != 'DUMMY'
           <!-- end 20191025_005 -->
        ;
        
        SELECT
			CASE 
				WHEN COUNT(SHIFT_CD) = 0 THEN 'S'
				ELSE 'E' 
			END  
			INTO #{p_err_code, mode=OUT, jdbcType=VARCHAR, javaType=String}
		  FROM MOM_SHIFT_SCHEDULE S
		 WHERE S.DIVISION_CD = #{divisionCd, jdbcType=VARCHAR}
		   AND S.COMPANY_CD  = #{companyCd, jdbcType=VARCHAR}
		   AND S.RESOURCE_CD = #{resourceCd, jdbcType=VARCHAR}
		   AND S.APPLY_DATE = TO_DATE(#{applyDate, jdbcType=VARCHAR}, 'YYYY-MM-DD')
		   <!-- start 20191025_005 -->
		   AND S.SHIFT_CD IN (SELECT SHIFT_CD
					          FROM   MOM_SHIFT_CODE C
					          WHERE  C.DIVISION_CD = S.DIVISION_CD
					          AND    C.COMPANY_CD = S.COMPANY_CD
					          AND    C.SHIFT_TYPE = #{shiftType, jdbcType=VARCHAR}
					          AND	 C.USE_YN = 'Y')
           AND S.SHIFT_CD != 'DUMMY'
           <!-- end 20191025_005 -->
		;
	 END
	}
