        ;
	  	
		DELETE FROM MOM_SHIFT_SCHEDULE S
         WHERE S.DIVISION_CD	 = #{divisionCd, jdbcType=VARCHAR}
           AND S.COMPANY_CD  	 = #{companyCd, jdbcType=VARCHAR}
           AND S.RESOURCE_CD	 = #{resourceCd, jdbcType=VARCHAR}
