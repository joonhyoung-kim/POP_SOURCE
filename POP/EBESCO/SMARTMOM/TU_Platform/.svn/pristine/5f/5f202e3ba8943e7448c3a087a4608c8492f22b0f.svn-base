		   					  FROM MOM_SHIFT_CODE
		   					 WHERE DIVISION_CD = #{divisionCd, jdbcType=VARCHAR}
		   					   AND COMPANY_CD = #{companyCd, jdbcType=VARCHAR}
		   					   AND SHIFT_TYPE = #{shiftType, jdbcType=VARCHAR})
		;
	  	
		INSERT INTO MOM_SHIFT_SCHEDULE
	        ( DIVISION_CD
