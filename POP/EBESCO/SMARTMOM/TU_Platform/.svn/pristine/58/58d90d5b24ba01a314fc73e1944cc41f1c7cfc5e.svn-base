			menu=JSON.parse(menuList);
			myMenu=authMenuList.replace('[','').replace(']','').replace(/\"/gi, "");
			myMenuArray=myMenu.split(',');
			$('#loginId').text(userId+"님");
			for(var i=0;i<menu.length;i++)			
				for(var j=0;j<myMenuArray.length;j++)				
